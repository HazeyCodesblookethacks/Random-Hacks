javascript:(function HistoryFlooderByHazey() {var introductionMessage="HISTORY FLOODER BY HAZEY - This tool will duplicate the current page in your browser's session history multiple times.";alert(introductionMessage);var promptMessage="Please enter the total number of times you want this page to appear in your session history (must be a number greater than 0):";var userInput=prompt(promptMessage);var numberOfTimesToDuplicate=parseInt(userInput);var isInvalidInput=!numberOfTimesToDuplicate||isNaN(numberOfTimesToDuplicate)||numberOfTimesToDuplicate<1;if(isInvalidInput){var errorMessage="ERROR: You did not enter a valid number. Please enter a number greater than 0 to continue.";alert(errorMessage);return}var currentPageURL=window.location.href;for(var currentIteration=1;currentIteration<=numberOfTimesToDuplicate;currentIteration++){var urlToPush=currentIteration===numberOfTimesToDuplicate?currentPageURL:currentIteration.toString();history.pushState({}, "Duplicated Entry #" + currentIteration, urlToPush);console.log("Added history entry " + currentIteration + " with URL: " + urlToPush)}var successMessage="SUCCESS! The current page has now been duplicated "+numberOfTimesToDuplicate+" "+(numberOfTimesToDuplicate===1?"time":"times")+" in your session history. You can now use your back and forward buttons to see the duplicated entries.";alert(successMessage);console.log("History flooding operation completed successfully. Thank you for using HISTORY FLOODER BY HAZEY.");})();
